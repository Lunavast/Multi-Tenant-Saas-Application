               <li data-toggle=\"modal\" data-target=\"#addCard\"><a href=\"#\" ><h5>Add Card</h5></a></li>\n                </div>\n            </div>\n            \n        <div class=\"modal fade\" id=\"status\" tabindex=\"-1\" role=\"dialog\" aria-labelledby=\"lstatus\" aria-hidden=\"true\">\n      <div class=\"modal-dialog\">\n        <div class=\"modal-content\">\n          <div class=\"modal-header\">\n            <button type=\"button\" class=\"close\" data-dismiss=\"modal\" aria-label=\"Close\">\n              <span aria-hidden=\"true\">&times;</span>\n            </button>\n            <h4 class=\"modal-title\" id=\"lstatus\">Project Status</h4>\n          </div>\n          <div class=\"modal-body\">\n              <div>\n              <label>total no of cards present in boards are :-</label>\n              </div>\n              <div>\n            <label>{{cardNumber}}</label>\n            </div>\n          </div>\n          <div class=\"modal-footer\">\n            <button type=\"button\" class=\"btn btn-default\" data-dismiss=\"modal\">OK</button>\n          </div>\n        </div>\n      </div>\n    </div>\n  \n            \n        <!-- card details popup -->    \n        <div class=\"modal fade\" id=\"addCard\" tabindex=\"-1\" role=\"dialog\" aria-labelledby=\"lstatus\" aria-hidden=\"true\">\n          <div class=\"modal-dialog\">\n            <div class=\"modal-content\">\n              <div class=\"modal-header\">\n                <button type=\"button\" class=\"close\" data-dismiss=\"modal\" aria-label=\"Close\">\n                <span aria-hidden=\"true\">&times;</span>\n                </button>\n                <h4 class=\"modal-title\" id=\"lstatus\">Card</h4>\n              </div>\n        <div class=\"modal-body\">\n              <label>Card Description</label>\n        <textarea rows=\"3\" class=\"form-control\" name=\"cardDescription\" ng-model=\"cardDescription\" placeholder=\"Enter the card description\"/></textarea>\n              </div>\n              <div class=\"modal-footer\">\n                <button type=\"button\" ng-click=\"postCard()\" class=\"btn btn-primary\">Save</button>\n              <button type=\"button\" class=\"btn btn-default\" ng-click=\"clearProject()\" data-dismiss=\"modal\">Cancel</button>\n              </div>\n            </div>\n          </div>\n        </div>\n            <!--sütun bitiş-->\n            <!--sütun başlangıç-->\n            <div class=\"panel panel-primary kanban-col\">\n                <div class=\"panel-heading\">\n                    DOING\n                    <i class=\"fa fa-2x fa-plus-circle pull-right\"></i>\n                </div>\n                <div class=\"panel-body\">\n                    <div id=\"DOING\" class=\"kanban-centered\">\n                    </div>\n                </div>\n            </div>\n            <!--sütun bitiş-->\n            <!--sütun başlangıç-->\n            <div class=\"panel panel-primary kanban-col\">\n                <div class=\"panel-heading\">\n                    DONE\n                    <i class=\"fa fa-2x fa-plus-circle pull-right\"></i>\n                </div>\n                <div class=\"panel-body\">\n                    <div id=\"DONE\" class=\"kanban-centered\">\n                    </div>\n                </div>            </div>\n            <!--sütun bitiş-->\n\n\n        </div>\n    </div>\n\n <div class=\"modal fade\" id=\"project\" tabindex=\"-1\" role=\"dialog\" aria-labelledby=\"lprojectname\" aria-hidden=\"true\">\n      <div class=\"modal-dialog\">\n        <div class=\"modal-content\">\n          <div class=\"modal-header\">\n            <button type=\"button\" class=\"close\" data-dismiss=\"modal\" aria-label=\"Close\">\n              <span aria-hidden=\"true\">&times;</span>\n            </button>\n            <h4 class=\"modal-title\" id=\"lprojectname\">Project Properties</h4>\n          </div>\n          <div class=\"modal-body\">\n            <label>Project Name</label>\n            <br>\n            <input type=\"text\" name='projectname' ng-model=\"projectname\" class=\"form-control\"/>\n            <br>\n            <label>Start Date</label>\n            <input type=\"date\" class=\"form-control\" name=\"projectStart\" ng-model=\"startDateProject\"  placeholder=\"Enter the Start Date of Project\" required/>\n            <br>\n            <label>Finish Date</label>\n           <input type=\"date\" class=\"form-control\" name=\"projectEnd\" ng-model=\"endDateProject\"  placeholder=\"Enter the Start Date of Project\" />\n          </div>\n          <div class=\"modal-footer\">\n            <button type=\"button\" ng-click=\"updateproject()\" class=\"btn btn-primary\">Save</button>\n            <button type=\"button\" class=\"btn btn-default\" ng-click=\"clearProject()\" data-dismiss=\"modal\">Cancel</button>\n          </div>\n        </div>\n      </div>\n    </div>\n    <!-- Static Modal -->\n    <div class=\"modal modal-static fade\" id=\"processing-modal\" role=\"dialog\" aria-hidden=\"true\">\n        <div class=\"modal-dialog\">\n            <div class=\"modal-content\">\n                <div class=\"modal-body\">\n                    <div class=\"text-center\">\n                        <i class=\"fa fa-refresh fa-5x fa-spin\"></i>\n                        <h4>Processing...</h4>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n    </div>\n    </div>\n     <script type=\"text/javascript\" src=\"javascripts/jquery-1.11.2.min.js\"></script>\n  <script type=\"text/javascript\" src=\"javascripts/bootstrap.min.js\"></script>\n    <!--<script src=\"angular/angular.min.js\"></script>-->\n    <script src=\"https://ajax.googleapis.com/ajax/libs/angularjs/1.3.15/angular.min.js\"></script>\n<script>\n            $(function () {\n            var kanbanCol = $('.panel-body');\n            kanbanCol.css('max-height', (window.innerHeight - 150) + 'px');\n\n            var kanbanColCount = parseInt(kanbanCol.length);\n            $('.container-fluid').css('min-width', (kanbanColCount * 350) + 'px');\n\n            draggableInit();\n\n            $('.panel-heading').click(function() {\n                var $panelBody = $(this).parent().children('.panel-body');\n                $panelBody.slideToggle();\n            });\n        });\n                            \n        function draggableInit() {\n            var sourceId;\n\n            $('[draggable=true]').bind('dragstart', function (event) {\n                sourceId = $(this).parent().attr('id');\n                event.originalEvent.dataTransfer.setData(\"text/plain\", event.target.getAttribute('id'));\n            });\n\n            $('.panel-body').bind('dragover', function (event) {\n                event.preventDefault();\n            });\n\n            $('.panel-body').bind('drop', function (event) {\n                var children = $(this).children();\n                var targetId = children.attr('id');\n\n                if (sourceId != targetId) {\n                    var elementId = event.originalEvent.dataTransfer.getData(\"text/plain\");\n\n                    $('#processing-modal').modal('toggle'); //before post\n\n\n                    // Post data \n                    setTimeout(function () {\n                        var element = document.getElementById(elementId);\n                        children.prepend(element);\n                        $('#processing-modal').modal('toggle'); // after post\n                    }, 1000);\n\n                }\n\n                event.preventDefault();\n            });\n        }\n\n</script>\n\n<script>\n    angular.module('rsc', []).controller('rscCtrl', function($scope,$http) {\n        $scope.projectname = \"Untitled\";\n        var tenantid;\n        var id=3;\n        var cardNo;\n      \n        $scope.saveProject = function() {\n        if($scope.projectname == \"Untitled\"){\n          alert('Project name cannot be untitled');\n        }\n        else{\n          alert('Project Saved Successfully!!!');\n        }\n      };\n      \n      \n      \n      var refresh = function(){\n        $http.get(\"/previousCardAdd\")\n        .success(function(response){\n            $scope.cards = response.doc;\n        $scope.tenantid = response.tenantid;\n                }).error(function(error){\n          alert(error);\n        });\n      };\n      \n      var cardNo;\n      \n      \n//      for (var k=1; k<=cardNo; cardNo--) {\n//          refresh();\n//      }\n      \n      var maxCard = function(){\n          $http.get(\"/fetchCardNo\")\n        .success(function(response){\n            console.log(response);\n            cardNo=response;\n            $scope.cardNumber=cardNo;\n            \n            //            $scope.cards = response.doc;\n//        $scope.tenantid = response.tenantid;\n                }).error(function(error){\n          alert(error);\n        });\n      };\n      \n     maxCard();\n     refresh();\n     \n     function cardStatus() {\n         \n         $scope.cardNumber=cardNo;\n     }\n//     refresh();\n  \n//         var refresh = function() {\n//     $http.get('/previousCardAdd').success(function(response) {\n//        console.log(\"I got the data I requested\");\n//        $scope.previousCardAdd = response;\n//        $scope.newCard = \"\";\n//      });\n//    };\n//    \n//    refresh();\n//     var j;\n//      if (cardNo != 0){\n//          j =cardNo;\n//      }\n//      else {\n//          j = 0;\n//      }\n//      \n//      for (var k=0; k<=j; j--) {\n//          refresh();\n//      }\n//      }\n        var cardValue=0;\n        $scope.postCard = function() {\n            cardValue++;\n          $scope.cardNumber=$scope.cardNumber+1;\n            //alert($scope.cardNumber);\n            console.log($scope.cardDescription);\n            $http({\n          method: 'POST',\n          url: '/cardDetails',\n             data:{ \n                \"projectid\": id,\n                        \"tenantid\": $scope.tenantid,\n              \"cardDescription\": $scope.cardDescription,\n              \"cardNo\": $scope.cardNumber\n            }   \n        })\n        .success(function(response){\n\n            console.log(response.body);\n            //alert(cardValue);\n//            refresh();\n            if (cardValue==1) {\n                        $.each(response, function() {  \n               $scope.Kanban1=this.cardDescription;\n                                                    }); }\n            else if (cardValue==2) {\n                               $.each(response, function() {  \n               $scope.Kanban2=this.cardDescription;\n                                       }); }\n            else if (cardValue==3) {\n                               $.each(response, function() {  \n               $scope.Kanban3=this.cardDescription;\n                                       }); }\n            else if (cardValue==4) {\n                               $.each(response, function() {  \n               $scope.Kanban4=this.cardDescription;\n                                       }); }\n            else if (cardValue==5) {\n                               $.each(response, function() {  \n               $scope.Kanban5=this.cardDescription;\n                                       }); }\n            else if (cardValue==6) {\n                                $.each(response, function() {  \n               $scope.Kanban6=this.cardDescription;\n                                       }); }\n            else if (cardValue==7) {\n                                $.each(response, function() {  \n               $scope.Kanban7=this.cardDescription;\n                                       }); }\n            else if (cardValue==8) {\n                                $.each(response, function() {  \n               $scope.Kanban8=this.cardDescription;\n                                      }); }\n            else if (cardValue==9) {\n                                $.each(response, function() {  \n               $scope.Kanban9=this.cardDescription;\n                                       }); }\n           else if (cardValue==10) {\n                                $.each(response, function() {  \n               $scope.Kanban10=this.cardDescription;\n                                       }); }\n            else {\n                alert(\"reached the limit of new cards\");\n            }\n         $('#addCard').modal('hide');\n          $scope.cardDescription = \"\";\n          }).error(function(error){\n          alert(error);\n        }); \n      };\n\n                $scope.cancel = function(){ \n          $scope.cardDescription='';\n      };\n         \n          $scope.clearProject = function(){\n            $http.get(\"/getProjectDetails\")\n            .success(function(response){\n            $scope.projectname =response[0].projectname;\n           $scope.startDateProject= new Date(response[0].projectstart);\n          if (response[0].projectend == null){\n                        $scope.endDateProject=response[0].projectend;\n                     }\n                     else{\n                     $scope.endDateProject= new Date(response[0].projectend);}\n            }).error(function(error){\n              alert(error);\n        });\n       };\n       \n       var saveProjectdetail = function(){\n         $scope.projectname = \"Untitled\";\n         $scope.startDateProject = new Date();\n         $scope.endDateProject = \"\";\n                $http({\n                method: 'POST',\n                url: '/saveProject',\n                data:{\n                  \"projectid\": id,\n                  \"tenantid\":$scope.tenantid,\n                \"projectname\": $scope.projectname,\n                \"projectstart\": $scope.startDateProject,\n                \"projectend\": $scope.endDateProject\n                }\n                }).success(function(response){\n                     $scope.projectname =response.projectname;\n                     $scope.startDateProject= new Date(response.projectstart);\n                     if (response.projectend == null){\n                        $scope.endDateProject=response.projectend;\n                     }\n                     else{\n                     $scope.endDateProject= new Date(response.projectend);}\n                     //$('#project').modal('hide');\n                  }).error(function(error){\n                    alert(error);\n                  });\n      };\n      \n       var getProjectDetails = function(){\n        $http.get(\"/getProjectDetails\")\n        .success(function(response){\n          if(response.length > 0){\n           if(response[0].projectid == 3){\n               $scope.projectname =response[0].projectname;\n            $scope.startDateProject=new Date( response[0].projectstart);\n            if (response[0].projectend == null){\n                        $scope.endDateProject=response[0].projectend;\n                     }\n                     else{\n                     $scope.endDateProject= new Date(response[0].projectend);}\n           }\n           else{\n              saveProjectdetail();\n           }\n          }\n          else{\n            saveProjectdetail();\n          }\n        }).error(function(error){\n          alert(error);\n        });\n      };\n      \n      getProjectDetails(); \n      \n      var updateProjectDetails = function(){\n         $scope.projectname = \"Untitled\";\n         $scope.startDateProject = new Date();\n         $scope.endDateProject = \"\";\n                $http({\n                method: 'POST',\n                 url: '/updateProjectDetails/' +$scope.tenantid,\n                data:{\n                \"projectname\": $scope.projectname,\n                \"projectstart\": $scope.startDateProject,\n                \"projectend\": $scope.endDateProject\n                }\n                }).success(function(response){\n                     $scope.projectname =response.projectname;\n                     $scope.startDateProject= new Date(response.projectstart);\n                     if (response.projectend == null){\n                        $scope.endDateProject=response.projectend;\n                     }\n                     else{\n                     $scope.endDateProject= new Date(response.projectend);}\n                     //$('#project').modal('hide');\n                  }).error(function(error){\n                    alert(error);\n                  });\n      };\n      \n      $scope.updateproject  = function(){\n         var start=new Date ($scope.startDateProject);\n        var end=new Date ($scope.endDateProject);\n         var today = new Date();\n        if (start - today < 0){\n          alert('Start date should be future date');\n        }\n        else if (start - end > 0){\n          alert('Finish date cannot be less than start date');\n        }\n        else{\n        $http({\n                method: 'POST',\n                url: '/updateProjectDetails/' +$scope.tenantid,\n                data:{\n                \"projectname\": $scope.projectname,\n                \"projectstart\": $scope.startDateProject,\n                \"projectend\": $scope.endDateProject\n                }\n                }).success(function(response){\n                     $scope.projectname =response.projectname;\n                     $scope.startDateProject= new Date(response.projectstart);\n                     if (response.projectend == null){\n                        $scope.endDateProject=response.projectend;\n                     }\n                     else{\n                     $scope.endDateProject= new Date(response.projectend);}\n                     $('#project').modal('hide');\n                  }).error(function(error){\n                    alert(error);\n                  });\n        }\n      };\n          \n      });\n  </script>\n\n</body>\n</html>","undoManager":{"mark":-1,"position":22,"stack":[[{"group":"doc","deltas":[